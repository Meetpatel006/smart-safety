{
  "expo": {
    "name": "SIH Smart Safety",
    "slug": "smart-safety",
    "version": "1.0.0",
    "scheme": "sihsmart",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/splash.jpg",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash.jpg",
          "imageWidth": 200
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.sihsmartsafety"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ae5f5633-03c9-4f1b-899c-2980cd6b2ebe"
      }
    },
    "owner": "redrepter221"
  }
}
